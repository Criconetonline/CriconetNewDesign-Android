<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#CABBBBBB"/>
            <corners android:radius="@dimen/_50sdp" />
            <gradient
                android:angle="-90"
                android:centerColor="#CABBBBBB"
                android:endColor="#CABBBBBB"
                android:gradientRadius="@dimen/_50sdp"
                android:startColor="#CABBBBBB"
                android:type="linear" />

        </shape>
    </item>

    <item
        android:left="@dimen/_2sdp"
        android:right="0dp"
        android:top="@dimen/_2sdp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#EAEAEA"/>
            <corners android:radius="@dimen/_50sdp" />
        </shape>
    </item>



</layer-list>

